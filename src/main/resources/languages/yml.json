{
  "name": "YAML",
  "wrap": true,
  "rules": [
    {
      "name": "keyword",
      "pattern": "\\s*(?<=[\\n\\r])\\S+(?=:)|\\S+(?=:)"
    },
    {
      "name": "string",
      "pattern": "[\"'](?:[^\"\\\\]|\\\\.)*[\"']"
    },
    {
      "name": "constant",
      "pattern": "(?:\\s*|)true|false|null|~"
    }
  ]
}